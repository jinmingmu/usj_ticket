{
    "manifest_version": 2,
    "name": "Getting started example",
    "description": "This extension download the pdfs from a japanese website",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "http://www.usj-hp.jp/*",
        "storage"
    ]
}
